                </tr>
                <tr>
                    <td>:fullscreen</td>
                    <td>:focus</td>
                    <td>:hover</td>
                    <td>:indeterminate</td>
                    <td>:in-range</td>
                    <td>:invalid</td>
                </tr>
                <tr>
                    <td>:lang()</td>
                    <td>:last-child</td>
                    <td>:last-of-type</td>
                    <td>:left</td>
                    <td>:link</td>
                    <td>:not()</td>
                </tr>
                <tr>
                    <td>:nth-child()</td>
                    <td>:nth-last-child()</td>
                    <td>:nth-last-of-type()</td>
                    <td>:nth-of-type()</td>
                    <td>:only-child</td>
                    <td>:only-of-type</td>
                </tr>
                <tr>
                    <td>:optional</td>
                    <td>:out-of-range</td>
                    <td>:read-only</td>
                    <td>:read-write</td>
                    <td>:required</td>
                    <td>:right</td>
                </tr>
                <tr>
                    <td>:root</td>
                    <td>:scope</td>
                    <td>:target</td>
